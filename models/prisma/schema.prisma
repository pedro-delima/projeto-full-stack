
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Aluno {
  id    Int     @id @default(autoincrement())
  nome  String
  email String  @unique
  idade Int
}

model Professor {
  id    Int     @id @default(autoincrement())
  nome  String
  email String  @unique
  idade Int
}
